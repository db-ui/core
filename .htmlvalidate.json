{
	"extends": ["html-validate:document", "html-validate:recommended"],
	"rules": {
		"require-sri": ["error", { "target": "crossorigin" }],
		"no-trailing-whitespace": ["off"],
		"doctype-style": ["off"],
		"element-name": ["error", { "whitelist": ["dialog"] }],
		"missing-doctype": ["off"],
		"no-dup-id": ["off"]
	}
}
